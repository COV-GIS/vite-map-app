{
  "name": "vite-map-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint",
    "postinstall": "node ./scripts/postinstall.js",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "serve": "npm run build && vite preview",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COV-GIS/vite-map-app.git"
  },
  "author": "COV-GIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/COV-GIS/vite-map-app/issues"
  },
  "homepage": "https://github.com/COV-GIS/vite-map-app#readme",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "ansi-regex": ">=6.1.0",
    "chalk": "^5.3.0",
    "eslint": "^9.12.0",
    "fs-extra": "^11.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.79.5",
    "tslib": "^2.8.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.9.0",
    "vite": "5.4.9"
  },
  "dependencies": {
    "@vernonia/core": "^4.1.4"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
