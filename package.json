  "homepage": "https://github.com/COV-GIS/vite-map-app#readme",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/luxon": "^3.4.2",
    "@types/papaparse": "^5.3.14",
    "ansi-regex": ">=6.0.1",
    "chalk": "^5.3.0",
    "eslint": "^9.9.0",
    "fs-extra": "^11.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tslib": "^2.6.3",
